    function addtocart(P_ID){
       var qty =  document.getElementById('sst');
        $.ajax({
         url: "@Url.Action("addtocart", "Home")",
        data: { id: P_ID, qty: qty.value },
        type: "POST",
        dataType: "json",

        success: function (res) {
        console.info(res);
            toastr.success('Item Added to Cart!');
            setTimeout(function () {
                window.location.reload();
            }, 3000);

        }
        });
        }

        function increse_pro_quantity(P_ID){
        $.ajax({
        url: "@Url.Action("cartincrese", "Home")",
        data: { id : P_ID },
        type: "POST",
        dataType: "json",

            success: function (res) {
                toastr.success('Item Quantity Increased!');
                setTimeout(function () {
                    window.location.reload();
                }, 3000);
        }
        });
        }

    function dcs_pro_quantity(P_ID) {
