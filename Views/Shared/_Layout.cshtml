<script>
    function myFunction() {
        var input, filter, ul, li, a, i, txtValue;
        input = document.getElementById("myInput");
        filter = input.value.toUpperCase();
        ul = document.getElementById("myUL");
        li = ul.getElementsByClassName("col-lg-4");

        for (i = 0; i < li.length; i++) {
            a = li[i].getElementsByClassName("title")[0]; // Targeting the title anchor tag
            txtValue = a.textContent || a.innerText;

            if (txtValue.toUpperCase().indexOf(filter) > -1) {
                li[i].style.display = ""; // Show the item if it matches the search
            } else {
                li[i].style.display = "none"; // Hide the item if it doesn't match
            }
        }
    }
